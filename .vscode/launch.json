	"version": "0.2.0",
	"configurations": [
		{
			"name": "--- Tests ---------------------",
		},
		{
			"name": "100) jsonstor-memory Tests",
			"type": "node",
			"request": "launch",
			"skipFiles": [
				"<node_internals>/**"
			],
			"cwd": "${workspaceFolder}",
			"program": "${workspaceFolder}/node_modules/mocha/bin/mocha",
			"args": [
				"-u",
				"bdd",
				"test/100*.js",
				"--timeout",
				"0",
				"--slow",
				"10",
				"--colors",
			],
			"outputCapture": "std",
			"internalConsoleOptions": "openOnSessionStart",
		},
